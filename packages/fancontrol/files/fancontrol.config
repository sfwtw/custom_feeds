config fancontrol settings
    option enable '1'
    option thermal_file '/sys/devices/virtual/thermal/thermal_zone0/temp'
    option fan_file '/sys/devices/platform/pwm-fan/hwmon/hwmon2/pwm1'
    option modem_mode '0'
    option start_temp '40'
    option start_speed '40'
    option max_speed '255'
    option temp_div '1000'
